{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "pip-compile": {
    "enabled": true
  },
  "pip_requirements": {
    "enabled": false
  },
  "pip_setup": {
    "enabled": false
  },
  "constraints": {
    "python": "3.12"
  },
  "prConcurrentLimit": 5,
  "prHourlyLimit": 2,
  "schedule": [
    "before 9am every day"
  ]
}
